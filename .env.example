# Server Configuration
PORT=3000

# Environment
NODE_ENV=development

# API Configuration
API_PREFIX=/api

# Swagger Documentation
SWAGGER_ENABLED=true
SWAGGER_PATH=/docs

# Client authentication secret (JWT used for client requests)
CLIENT_SECRET=myclientsecret

# Server authentication secret (signature for server-to-server communication)
SERVER_SIGNATURE_SECRET=myserversecret

# Add other environment variables as needed
# DATABASE_URL=
# JWT_SECRET=
# etc. 